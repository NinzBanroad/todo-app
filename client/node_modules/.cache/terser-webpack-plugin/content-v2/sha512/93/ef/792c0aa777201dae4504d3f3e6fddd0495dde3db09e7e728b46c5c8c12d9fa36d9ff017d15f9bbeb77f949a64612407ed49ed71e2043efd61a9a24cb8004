            if (res.data === 'Update Added!') {\n                    this.setState({ open: false });\n                    window.location = '/'\n                }\n            })\n\n    }\n\n    render() {\n        return (\n            <>\n                <table class=\"table table-hover\">\n                    <thead>\n                        <tr>\n                            <th scope=\"col\">#</th>\n                            <th scope=\"col\">Todo</th>\n                            <th scope=\"col\">Action</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {\n                            this.state.todos.map((data, i) =>\n                                <tr>\n                                    <th scope=\"row\">{i}</th>\n                                    <td>{data.todo}</td>\n                                    <td style={{ cursor: 'pointer' }}>\n                                        <button type=\"button\" onClick={(e) => this.onEditTodo(e, data)} class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#exampleModal\"><i class=\"fas fa-edit\"></i></button>&nbsp;&nbsp;&nbsp;\n                                    <button class=\"btn btn-danger\" onClick={(e) => this.onDeleteTodo(e, data._id)}><i class=\"fas fa-trash\"></i></button>\n                                    </td>\n                                </tr>\n                            )\n                        }\n                    </tbody>\n                </table>\n                {\n                    this.state.todo &&\n                    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n                    <div class=\"modal-dialog\" role=\"document\">\n                        <div class=\"modal-content\">\n                            <div class=\"modal-header\">\n                                <h5 class=\"modal-title\" id=\"exampleModalLabel\">Update Todo</h5>\n                                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                                    <span aria-hidden=\"true\">&times;</span>\n                                </button>\n                            </div>\n                            <div class=\"modal-body\">\n                                <form onSubmit={this.onSubmit}>\n                                    <div class=\"form-group\">\n                                        <label for=\"exampleInputEmail1\">Todo</label>\n                                        <input type=\"text\" value={this.state.todo} name=\"todo\" class=\"form-control\" onChange={(e) => this.onChange(e)} placeholder=\"Enter task here\" />\n                                    </div>\n                                    <button type=\"submit\" value=\"Submit\" className=\"btn btn-primary\" class=\"btn btn-primary\">Submit</button>\n                                </form>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                }\n            </>\n        )\n    }\n}","import React, { Component } from 'react'\nimport axios from 'axios'\n\nexport default class AddTodo extends Component {\n    constructor(props) {\n\n        super(props);\n\n        this.state = {\n            isLoading: false,\n            todo: ''\n        }\n\n        this.onChange = this.onChange.bind(this);\n\t\tthis.onSubmit = this.onSubmit.bind(this)\n    }\n\n    onChange(todo) {\n        this.setState({ todo: todo })\n    }\n\n\n    onSubmit(e) {\n        e.preventDefault();\n        let data = {\n            todo: this.state.todo\n        }\n        axios.post('http://localhost:5000/todoApp/add', data)\n            .then(res => {\n                if (res.data === 'Todo Added!') {\n                    this.setState({ isLoading: false, error: '' });\n                    window.location = '/'\n  